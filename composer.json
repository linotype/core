{
    "name": "linotype/linotype",
    "type": "library",
    "description": "a component monolith",
    "keywords": [
        "linotype",
        "core"
    ],
    "homepage": "https://linotype.dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Armspach",
            "email": "yannick.armspach@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Linotype\\Core\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/filesystem": "^4.4|^5.0",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/yaml": "5.2.*",
        "linotype/installers": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2",
        "symfony/var-dumper": "^4.4.17|^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
